@using Newtonsoft.Json.Linq
@using DHIS2Xfer.Models.Job

@model UnMapped

@{

}

<div class="input-group" style="margin-bottom:5px;">
    <input type="text" class="form-control" placeholder="Search..." id="@Model.ID" onkeyup="search('@Model.ID')">
    <div class="input-group-append">
        <button class="btn btn-secondary" type="button">
            <i class="fa fa-search"></i>
        </button>
    </div>
</div>

<select class="form-control" size="10" id="@Model.SelectID" onclick="loadOptionData('@Model.SelectID','@Model.Type')">
    @foreach (JObject d in Model.Data)
    {
        if (d.GetValue("type").ToString() == Model.Type)
        {
            <!option value="@d.GetValue("id")" title="@d.GetValue("displayName")">@d.GetValue("displayName")</!option>
        }
    }
</select>

<br />

@{ 
    string labelID = Model.SelectID + "Label";
    string optionID = Model.SelectID + "Option";
}
<label id="@labelID" for="umDestOption" style="font-weight:bold;">Options</label>
<select class="form-control" id="@optionID">
    <!option value="na">NA</!option>
</select>